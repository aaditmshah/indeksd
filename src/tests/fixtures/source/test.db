type Value = 1 | 2 | 3;
type Value2 = '1' | true | 0.2;
type Value3 = Value | false;
type Value4 = 'some';

database MyDatabase {

  table Authors {
    @autoincrement id: number;
    @index name: string;
    @index @unique email: string;
  }

  table Posts @item("Post") {
    @index name: string;
    content: string;
  }

}